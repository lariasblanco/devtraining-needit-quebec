        <xml><![CDATA[<?xml version="1.0" encoding="utf-8" ?>
<j:jelly trim="false" xmlns:j="jelly:core" xmlns:g="glide" xmlns:j2="null" xmlns:g2="null">

<g:evaluate var="jvar_file_types">
  var types = new GlideRecord("sys_embed_object");
  types.addActiveQuery();
  types.query();
  var answer = "";
  while (types.next()) {
    if (answer != "")
      answer += ", ";
    answer += "." + types.extension;
  }
  answer;
</g:evaluate>
  <tr>
    <td colspan="2">${gs.getMessage("Supported file types")}: ${jvar_file_types}</td>
  </tr>
</j:jelly>]]></xml>
